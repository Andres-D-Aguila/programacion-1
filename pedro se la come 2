#include <iostream>
#include <conio.h>

using namespace std;

void impares(int n)
{
	int a=1,i;
	for(i=0;i<n;i++)
	{
		cout<<a<<"	";
		a=a+2;
	}
}

void parimpar(int n)
{
	int a=2,b=1,k=0,i;
	for(i=0;i<n;i++)
	{
		if(k==0)
		{
			cout<<a<<"	";
			a=a+2;
			k=1;
		}
		else
		{
			cout<<b<<"	";
			b=b+2;
			k=0;
		}
	}
}

void cuadrados(int n)
{
	int a=1,i;
	for(i=0;i<n;i++)
	{
		cout<<a*a<<"	";
		a=a+1;
	}
}

void abc(int n)
{
	int i,k=0;
	for(i=0;i<n;i++)
	{
		if(k==0)
		{
			cout<<"a	";
			k=1;
		}
		else
		{
			if(k==1)
			{
				cout<<"b	";
				k=2;
			}
			else
			{
				cout<<"c	";
				k=0;
			}
		}
	}
}

void masuno(int n)
{
	int a=1,i;
	for(i=1;i<=n;i++)
	{
		cout<<a<<"	";
		a=a+i;
	}
}

void fibonacci(int n)
{
	int ant=0,act=1,sig,i;
	for(i=0;i<n;i++)
	{
		cout<<act<<"	";
		sig=act+ant;
		ant=act;
		act=
	}
}

void main()
{
	int n;
	cout<<"ingrese un numero: ";
	cin>>n;
	impares(n);
	cout<<endl;
	parimpar(n);
	cout<<endl;
	cuadrados(n);
	cout<<endl;
	abc(n);
	cout<<endl;
	masuno(n);
	cout<<endl;
	fibonacci(n);
	getch();
}
